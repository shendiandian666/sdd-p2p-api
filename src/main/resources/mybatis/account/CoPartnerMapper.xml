<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sdd.mapper.CoPartnerMapper">

	
	<select id="coPartnerList" resultType="java.util.Map">
		  select * from p2p_co_partner a,p2p_platform b
		  where a.platform_id = b.id
		  order by orderby
	</select>
   
</mapper>